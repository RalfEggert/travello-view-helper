{
    "name": "ralfeggert/travello-view-helper",
    "description": "Zend Framework 3 module with a couple of useful view helper",
    "license": "The MIT License (MIT)",
    "authors": [
        {
            "name": "Ralf Eggert",
            "email": "ralf@travello.de",
            "homepage": "http://www.travello.de",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.6 || ^7.0",
        "ext-intl" : "*",
        "zendframework/zend-mvc": "^3.0",
        "zendframework/zend-mvc-plugin-flashmessenger": "^1.0",
        "zendframework/zend-form": "^2.8 || ^3.0"
    },
    "extra": {
        "zf": {
            "component": "TravelloViewHelper",
            "config-provider": "TravelloViewHelper\\ConfigProvider"
        }
    },
    "autoload": {
        "psr-4": {
            "TravelloViewHelper\\": "src/"
        }
    }
}
